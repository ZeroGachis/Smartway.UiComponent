<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Smartway.UiComponent.Sample.Pages.ModalSample"
             x:Name="Self">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="ModalSettings FullHeight" Command="{Binding Source={x:Reference Self}, Path=ModalSettingsFullHeight}" />
            <Button Text="ModalSettings CustomHeight" Command="{Binding Source={x:Reference Self}, Path=ModalSettingsCustomHeight}" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>